<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/LinearLayout1"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/TableRow14"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal" >

        <CheckBox
            android:id="@+id/tv_checkupl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:gravity="center_horizontal|center_vertical" />

    </LinearLayout>

    <TableRow
        android:id="@+id/TableRow13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TableRow1"
        android:layout_below="@+id/TableRow1" >

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:text="@string/tgl" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sama_dengan" />

        <TextView
            android:id="@+id/tv_tgl_upl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/kosong" />
    </TableRow>

    <TableRow
        android:id="@+id/TableRow12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TableRow1"
        android:layout_below="@+id/TableRow13"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView3"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:text="@string/nama_cust" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sama_dengan" />

        <TextView
            android:id="@+id/tv_namacust_upl"
            android:layout_width="215dp"
            android:layout_height="wrap_content"
            android:text="@string/kosong" />
    </TableRow>

    <TableRow
        android:id="@+id/TableRow14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TableRow1"
        android:layout_below="@+id/TableRow12" >

        <TextView
            android:id="@+id/textView5"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:text="@string/jml_order" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sama_dengan" />

        <TextView
            android:id="@+id/tjml1_ord_upl"
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="@string/kosong_number" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/pemisah" />

        <TextView
            android:id="@+id/tjml_ord2_upl"
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:gravity="right"
            android:text="@string/kosong_number" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/pemisah" />

        <TextView
            android:id="@+id/tjml_ord3_upl"
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="@string/kosong_number" />
    </TableRow>

    <TableRow
        android:id="@+id/TableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/LinearLayout1"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tuser_detail"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:text="@string/no_pengajuan" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sama_dengan" />

        <TextView
            android:id="@+id/tv_nopeng_upl"
            android:layout_width="215dp"
            android:layout_height="wrap_content"
            android:text="@string/kosong" />
    </TableRow>

    <TextView
        android:id="@+id/tv_carabayar_upl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/TableRow1"
        android:gravity="center_vertical"
        android:text="@string/kosong"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>